class Book {
    String title;
    int price;

    Book(String title, int price) {
        this.title = title;
        this.price = price;
    }

    void printInfo() {
        System.out.println(title + " - " + price + "원");
    }
}

public class Main {
    public static void main(String[] args) {
        Book b = new Book("혼공자", 25000);
        b.printInfo();
    }
}
